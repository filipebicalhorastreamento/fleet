{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.3",
        "edujugon/push-notification": "^5.1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kodeine/laravel-meta": "dev-master",
        "kreait/laravel-firebase": "^5.1",
        "laravel-notification-channels/webpush": "^6.0.0",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.4",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^10.1",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "6.*",
        "maatwebsite/excel": "^3.1",
        "minishlink/web-push": "^6.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.0",
        "pusher/pusher-php-server": "^5.0",
        "razorpay/razorpay": "2.*",
        "sly/notification-pusher": "*",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-permission": "^5.9.1",
        "stripe/stripe-php": "^7.90",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
            
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
